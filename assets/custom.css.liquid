.basel-back-btn>span {
    width: 25px;
    height: 3px;
    line-height: 3px;
    background-color: #5f0933;
}
.basel-back-btn>span:before {
    left: -4px;
    top: 0;
    font-size: 20px;
    line-height: 3px;
    color: #5f0933;
}
.basel-back-btn>span:after {
    top: -6px;
    right: -3px;
    width: 3px;
    height: 9px;
    line-height: 0;
    background-color: #5f0933;
}
.basel-products-nav .product-btn>a span {
    height: 3px;
    background-color: #5f0933;
}
.basel-products-nav .product-btn>a span:before {
    font-size: 18px;
    line-height: 5px;
    color: #5f0933;
}
.basel-products-nav .product-short {box-shadow: 0px 5px 5px -3px rgba(0, 0, 0, 0.2), 0px 8px 10px 1px rgba(0, 0, 0, 0.14), 0px 3px 14px 2px rgba(0, 0, 0, 0.12); background-color: #f9f9f9; border: #f2f2f2 solid 2px; border-radius: 2px;}

